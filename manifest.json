{
  "manifest_version": 3,
  "name": "FocusTab",
  "version": "1.0",
  "description": "A personalized productivity dashboard for your Chrome new tab.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action":{
        "default_popup": "index.html",
        "default_icon": "images/image.png"
  },
  "permissions": ["storage", "notifications"],
  "icons": {
    "128": "images/image.png"
  }

}

